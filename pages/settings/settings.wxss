.header{
  display: flex;
  flex-direction: column;
}

.space-between {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100rpx;
  border-style: solid;
  border-width: 2rpx;
  border-radius: 50rpx;
  border-color: #000000;
  background-color: #ffffff;
  margin: 20rpx;
}

.font{
  color: #000000;
  font-size: 30rpx;
  align-items: center;
  padding: 20rpx;
}

.slider{
  width: 450rpx;
}

.switch{
  position: fixed;
  right: 30rpx;
}

.button{
  height: 100rpx;
  border-style: solid;
  border-width: 2rpx;
  border-radius: 50rpx;
  border-color: #000000;
  background-color: #ffffff;
  margin: 20rpx;
}

.buttonpaired{
  height: 100rpx;
  border-style: solid;
  border-width: 2rpx;
  border-radius: 50rpx;
  border-color: rgb(228,228,228);
  color: rgb(228,228,228);
  background-color: #ffffff;
  margin: 20rpx;
}

.buttondeletepaired{
  height: 100rpx;
  border-style: solid;
  border-width: 2rpx;
  border-radius: 50rpx;
  border-color: rgb(255,0,0);
  color: rgb(255,0,0);
  background-color: #ffffff;
  margin: 20rpx;
}